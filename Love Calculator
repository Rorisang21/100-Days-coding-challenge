print("The Love Calculator is calculating your score...")
name1 = input("Please insert the first name. \n") # What is your name?
name2 = input("Please insert the second name. \n") # What is their name?
# ðŸš¨ Don't change the code above ðŸ‘†
# Write your code below this line ðŸ‘‡
name1= list(name1.lower())
name2= list(name2.lower())
name= name1  + name2
t_times= name.count('t')
r_times= name.count('r')
u_times= name.count('u')
e_times= name.count('e')
total_true1 = str(t_times + r_times + u_times + e_times)

L_times = name.count('l')
O_times = name.count('o')
V_times= name.count('v')
E_times = name.count('e')
total_times2 = str(L_times + O_times + V_times + E_times)

love_score=(total_true1 + total_times2)
love_score=int(love_score)

if love_score <10 or love_score > 90:
  print(f"Your score is {love_score}, you go together like coke and mentos.")
elif love_score >=40 and love_score <= 50:
  print (f"Your score is {love_score}, you are alright together.")

else:
  print(f"Your score is {love_score}.")

